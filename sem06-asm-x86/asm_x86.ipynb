    "        .string \"HERE: %d \\n\"  /* DELETE IT */\n",
    "merge:\n",
    "        // stack :: adr, arr, left, mid, right\n",
    "        push ebp\n",
    "        push edi\n",
    "        push esi\n",
    "        push ebx\n",
