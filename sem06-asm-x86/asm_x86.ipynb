   "source": [
    "%%asm clamp_if.S\n",
    ".intel_syntax noprefix\n",
    ".text\n",
    ".globl clamp\n",
    "clamp:\n",
    "    mov edx, DWORD PTR [esp + 4] // X\n",
    "    mov eax, DWORD PTR [esp + 8] // A\n",
    "    cmp edx, eax\n",
    "    jl return_eax // return A if X < A\n",
    "    mov eax, DWORD PTR [esp + 12] // B\n",
    "    cmp edx, eax\n",
    "    jg return_eax // return B if X > B\n",
    "    mov eax, edx\n",
    "return_eax:\n",
    "    ret"
   ]
