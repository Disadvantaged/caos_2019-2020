    "        lea ebx, DWORD PTR [esp + 4*ebx] /* ebx = result + it1 + it2]*/\n",
    "        mov DWORD PTR [ebx], eax /* result[it1+it2] = eax */\n",
    "        add esi, 1 /* ++it1*/\n",
    "        jmp cycle2\n",
    "\n",
    "\n",
    "\n",
    "\n",
