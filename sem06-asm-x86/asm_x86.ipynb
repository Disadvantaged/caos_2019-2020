    "%%asm clamp_disasm.S\n",
    ".intel_syntax noprefix\n",
    ".text\n",
    ".globl clamp\n",
    "clamp:\n",
    "    mov edx, DWORD PTR [esp+4]\n",
    "    mov eax, DWORD PTR [esp+8]\n",
    "    cmp edx, eax\n",
    "    jl .L2\n",
    "    cmp edx, DWORD PTR [esp+12]\n",
    "    mov eax, edx\n",
    "    cmovg eax, DWORD PTR [esp+12]\n",
    ".L2:\n",
    "    rep ret"
   ]
