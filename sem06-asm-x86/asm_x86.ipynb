    "        mov ebx, DWORD PTR [ebp + 8] /* ebx = left */\n",
    "        add ebx, esi /* ebx += it1 */\n",
    "        mov ebx, DWORD PTR [eax + 4*ebx] /* ebx = arr[ebx] = arr[ left + it1 ] */\n",
    "        mov ecx, DWORD PTR [ebp + 12] /* ecx = mid */\n",
    "        add ecx, edi /* ecx += it2*/\n",
    "        mov ecx, DWORD PTR [eax + 4*ecx] /* ecx = arr[ecx] = arr[ mid + it2] */\n",
