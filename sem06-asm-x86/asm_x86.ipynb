    "%%cpp simdiv.c\n",
    "%run gcc -m32 -masm=intel -O3 simdiv.c -o simdiv.exe\n",
    "%run ./simdiv.exe\n",
    "\n",
    "#include <stdint.h>\n",
    "#include <assert.h>\n",
    "    \n",
    "int32_t simdiv(int32_t a) { \n",
    "    uint32_t eax = ((uint32_t)a >> 31) + a;\n",
    "    __asm__(\"sar %0\" : \"=a\"(eax) : \"a\"(eax));\n",
    "    return eax;\n",
    "}\n",
    "\n",
    "int main() {\n",
    "    assert(simdiv(1) == 0);\n",
    "    assert(simdiv(5) == 2);\n",
    "    assert(simdiv(-1) == 0);\n",
    "    assert(simdiv(-5) == -2);\n",
    "}"
   ]
