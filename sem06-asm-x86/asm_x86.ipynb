    ".intel_syntax noprefix\n",
    ".text\n",
    ".globl my_sum\n",
    "my_sum:\n",
    "    push ebx\n",
    "    mov eax, 0\n",
    "    mov edx, DWORD PTR [esp + 8]\n",
    "    mov ebx, DWORD PTR [esp + 12]\n",
    "start_loop:\n",
    "    cmp edx, 0\n",
    "    jle return_eax\n",
    "    add eax, DWORD PTR [ebx]\n",
    "    add ebx, 4\n",
    "    dec edx\n",
    "    \n",
    "    cmp edx, 0\n",
    "    jle return_eax\n",
    "    sub eax, DWORD PTR [ebx]\n",
    "    add ebx, 4\n",
    "    dec edx\n",
    "    \n",
    "    jmp start_loop\n",
    "return_eax:\n",
    "    pop ebx\n",
    "    ret"
